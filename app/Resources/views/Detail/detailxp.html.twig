{% for cvitem in cvItems%}
<div class="row">
<div class="col-sm-2 col-sm-push-10 col-md-2 col-md-push-10  col-lg-2 col-lg-push-10">
	{{cvitem.date}}
	</div>	
	<div class="col-sm-10 col-sm-pull-2 col-md-10 col-md-pull-2 col-lg-3 col-lg-pull-2">	
		{% autoescape false %}
		{{cvitem.title}}
		{% endautoescape %}
	</div>
	
	<div class="col-md-12 col-md-pull-0 col-lg-7 col-lg-pull-2">
		<h3>{{cvitem.title2}}</h3>
		<p>{% autoescape false %}{{cvitem.body}}{% endautoescape %}</p>
	</div>
	
</div>
{%endfor%}	